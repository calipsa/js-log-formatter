{
  "name": "@calipsa/log-formatter",
  "version": "0.3.2",
  "description": "JavaScript log formatter",
  "private": false,
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "files": [
    "dist",
    "es"
  ],
  "scripts": {
    "test": "jest --no-cache",
    "lint": "eslint './src/**/*.ts'",
    "build:commonjs": "tsc --outDir dist",
    "buildwip:es": "tsc -m ESNext --outDir es",
    "build": "rm -rf dist && run-p -c \"build:*\"",
    "version": "npm run build",
    "release": "np"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/calipsa/js-log-formatter"
  },
  "keywords": [
    "js",
    "log"
  ],
  "author": "Anton Veselev",
  "license": "ISC",
  "dependencies": {
    "@calipsa/tz-utils": "^1.0.3",
    "chalk": "^4.1.0",
    "lodash": "^4.17.20"
  },
  "devDependencies": {
    "@calipsa/eslint-config-typescript": "^1.15.0",
    "@types/jest": "^26.0.20",
    "@types/lodash": "^4.14.168",
    "eslint": "^7.18.0",
    "jest": "^26.6.3",
    "np": "^6.5.0",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^26.5.0",
    "typescript": "^4.1.3"
  }
}
